local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "MT " .. Fluent.Version,
    SubTitle = "by rkqze",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
})

-- Fluent provides Lucide Icons, they are optional
local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "" }),
    Oyunlar = Window:AddTab({ Title = "Oyunlar", Icon = "" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}
    

Tabs.Main:AddButton({
    Title = "Evrensel 1.",
    Description = "1. Evrensel Script",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Maanaaaa/ManaV2ForRoblox/main/MainScript.lua"))()

    end
})

Tabs.Main:AddButton({
    Title = "Evrensel ESP",
    Description = "Tüm oyunlarda (koruması kötü olanlar) çalışır",
    Callback = function()
        loadstring(game:HttpGet("https://obj.wearedevs.net/140060/scripts/Universal%20ESP.lua"))()
    end
})

Tabs.Main:AddButton({
    Title = "Ucma",
    Description = "Eğer evrensel scriptlerden açmayı istemiyorsan buradan aç",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))()
    end
})

Tabs.Oyunlar:AddParagraph({
    Title = "Not:",
    Content = "Bu sekmede çoğu oyunun scriptleri bulunur eğer bozuk veya şifreli script varsa instagram @rkqze Eğer öneriniz varsa @rkqze İyi Oyunlar"
})

Tabs.Oyunlar:AddParagraph({
    Title = "Güncellemeler",
    Content = "güncelleme yakında"
})

local Library = loadstring(game:HttpGet('https://raw.githubusercontent.com/VisualRoblox/Roblox/main/UI-Libraries/Visual%20UI%20Library/Source.lua'))()

local Window = Fluent:CreateWindow({
    Title = "Fluent " .. Fluent.Version,
    SubTitle = "by dawid",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
})
